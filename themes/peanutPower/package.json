{
    "name": "custom_natalie",
    "version": "1.0.0",
    "description": "Theme for nataliech.in",
    "main": "gulpfile.js",
    "author": "Natalie Chin",
    "copyright": "Natalie Chin",
    "license": "ISC",
    "paths": {
        "src": {
            "base": "assets/src/",
            "scss": "assets/src/scss/",
            "css": "assets/src/css/",
            "fonts": "assets/src/fonts/",
            "json": "assets/src/json/",
            "js": "assets/src/js/",
            "img": "assets/src/img/",
            "svg": "assets/src/svg/"
        },
        "build": {
            "base": "assets/build/",
            "css": "assets/build/css/",
            "fonts": "assets/build/fonts/",
            "js": "assets/build/js/",
            "html": "assets/build/html/",
            "img": "assets/build/img/"
        },
        "dist": {
            "base": "source/",
            "css": "source/css/",
            "js": "source/js/",
            "fonts": "source/fonts/",
            "img": "source/img/",
            "svg": "source/svg/"
        },
        "templates": "templates/"
    },
    "urls": {
        "live": "",
        "dev": "",
        "critical": ""
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "printCssName": "print.min.css",
        "scssName": "styles.scss",
        "cssName": "styles.css",
        "jsName": "script.js",
        "buildJsName": "buildScript.js"
    },
    "globs": {
        "distCss": [
            "assets/build/css/main.css"
        ],
        "distPrintCss": [
            "assets/build/css/print.css"
        ],
        "distJs": [
            "node_modules/picturefill/dist/picturefill.min.js",
            "node_modules/fontfaceobserver/fontfaceobserver.js",
            "assets/build/js/**/hoverintent.js",
            "assets/build/js/**/*.js"
        ],
        "babelJs": [
            "assets/src/js/**/*.js"
        ]
    },
    "dependencies": {
        "fg-loadcss": "^1.2.0",
        "fontfaceobserver": "^2.0.5",
        "normalize.css": "^5.0.0",
        "picturefill": "^3.0.2",
        "sass-mq": "^3.3.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.16.0",
        "bootstrap-sass": "^3.3.7",
        "browser-sync": "^2.23.2",
        "chalk": "^1.1.3",
        "dcf": "git+https://github.com/d-c-n/dcf.git",
        "fancy-log": "^1.3.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.1.0",
        "gulp-babel": "^7.0.0",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.1",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^2.1.2",
        "gulp-download": "^0.0.1",
        "gulp-eslint": "^4.0.2",
        "gulp-filter": "^5.1.0",
        "gulp-header": "^1.8.7",
        "gulp-if": "^2.0.2",
        "gulp-ignore": "^2.0.2",
        "gulp-imagemin": "^3.1.1",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.5.0",
        "gulp-newer": "^1.2.0",
        "gulp-notify": "^3.1.0",
        "gulp-plumber": "^1.1.0",
        "gulp-postcss": "^7.0.1",
        "gulp-print": "^2.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-rev": "^7.1.0",
        "gulp-sass": "^3.1.0",
        "gulp-sass-glob": "^1.0.8",
        "gulp-sassvg": "^3.0.0",
        "gulp-sftp": "^0.1.5",
        "gulp-shell": "^0.6.5",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.6.3",
        "gulp-streamify": "1.0.2",
        "gulp-svg-sprite": "^1.3.7",
        "gulp-uglify": "^3.0.0",
        "gulp-util": "^3.0.8",
        "modularscale-sass": "^3.0.5",
        "moment": "^2.20.1",
        "path": "^0.12.7",
        "postcss-normalize": "^4.0.0",
        "vinyl-ftp": "^0.6.0",
        "vinyl-source-stream": "^1.1.2"
    },
    "browserslist": [
        "last 1 Chrome version",
        "last 1 Explorer version",
        "last 1 Firefox version",
        "Firefox ESR",
        "last 1 Safari version",
        "Android >= 4.0",
        "BlackBerry >= 4.0",
        "iOS >= 10"
    ]
}
